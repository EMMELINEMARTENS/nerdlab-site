{
  "require": {
    "craftcms/cms": "^4.0.0",
    "vlucas/phpdotenv": "^3.3",
    "craftcms/redactor": "3.0.4",
    "ether/seo": "4.2.2",
    "verbb/super-table": "3.0.12 ",
    "craftcms/ckeditor": "3.8.3",
    "mmikkel/cp-field-inspect": "1.4.4",
    "ether/simplemap": "4.0.5",
    "nystudio107/craft-minify": "4.0.1",
    "clubstudioltd/craft-asset-rev": "7.0.0",
    "craftcms/store-hours": " 3.1.0",
    "rias/craft-position-fieldtype": "4.0.0",
    "nystudio107/craft-cookies": "4.0.1",
    "xpertbot/craft-wheelform": "3.2.0",
    "topshelfcraft/wordsmith": "4.4.0",
    "topshelfcraft/supersort": "4.0.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
